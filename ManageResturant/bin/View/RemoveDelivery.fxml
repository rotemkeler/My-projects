<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.RemoveDeliveryController">
   <children>
      <AnchorPane fx:id="removeDeliveryScreen" layoutX="3.0" layoutY="3.0" prefHeight="500.0" prefWidth="500.0">
         <children>
            <Label layoutX="122.0" layoutY="77.0" prefHeight="40.0" prefWidth="255.0" text="Remove Delivery:" textFill="#efb81e">
               <font>
                  <Font name="System Bold Italic" size="28.0" />
               </font>
            </Label>
            <Label layoutX="66.0" layoutY="156.0" prefHeight="49.0" prefWidth="375.0" text="Please select a delivery from the list:">
               <font>
                  <Font size="23.0" />
               </font>
            </Label>
            <ComboBox fx:id="deliveriesList" layoutX="81.0" layoutY="250.0" prefHeight="25.0" prefWidth="339.0" />
            <Button fx:id="back" layoutX="14.0" layoutY="21.0" mnemonicParsing="false" onAction="#removeDeliverySceneToMainScene" prefHeight="25.0" prefWidth="65.0" text="Back" />
            <Button layoutX="217.0" layoutY="426.0" mnemonicParsing="false" onAction="#saveRemoveDelivery" prefHeight="25.0" prefWidth="65.0" text="Remove" />
            <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="345.0" layoutY="2.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/logo.PNG" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
