<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ComponentsListController">
   <children>
      <AnchorPane fx:id="componentsList" onDragDetected="#componentsListSceneToMainScene" prefHeight="500.0" prefWidth="500.0">
         <children>
            <ImageView fitHeight="534.0" fitWidth="591.0" layoutY="-17.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/1.jpg" />
               </image>
            </ImageView>
            <Button fx:id="back" layoutX="29.0" layoutY="22.0" mnemonicParsing="false" onAction="#componentsListSceneToMainScene" prefHeight="25.0" prefWidth="61.0" text="Back" />
            <Label layoutX="126.0" layoutY="19.0" prefHeight="55.0" prefWidth="247.0" text="Components:" textFill="#efb81e">
               <font>
                  <Font name="System Bold Italic" size="38.0" />
               </font>
            </Label>
            <ListView fx:id="allComponents" layoutX="29.0" layoutY="159.0" prefHeight="256.0" prefWidth="458.0" />
            <ImageView fitHeight="118.0" fitWidth="116.0" layoutX="394.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/logo.PNG" />
               </image>
            </ImageView>
            <Button fx:id="edit" layoutX="214.0" layoutY="434.0" mnemonicParsing="false" onAction="#editComp" onMouseClicked="#compChoose" text="Edit Comp" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
