<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="cooksList" prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.CooksListController">
   <children>
      <ImageView fitHeight="540.0" fitWidth="551.0" layoutX="-9.0" layoutY="-14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/1.jpg" />
         </image>
      </ImageView>
      <Button fx:id="back" layoutX="29.0" layoutY="22.0" mnemonicParsing="false" onAction="#cooksListSceneToMainScene" prefHeight="25.0" prefWidth="61.0" text="Back" />
      <Label layoutX="182.0" layoutY="19.0" prefHeight="55.0" prefWidth="136.0" text="Cooks:" textFill="#efb81e">
         <font>
            <Font name="System Bold Italic" size="38.0" />
         </font>
      </Label>
      <ListView fx:id="allCooks" layoutX="9.0" layoutY="142.0" prefHeight="267.0" prefWidth="485.0" />
      <ImageView fitHeight="125.0" fitWidth="123.0" layoutX="387.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/logo.PNG" />
         </image>
      </ImageView>
      <Button layoutX="216.0" layoutY="426.0" mnemonicParsing="false" onAction="#editCook" onMouseClicked="#cookChoose" text="Edit Cook" />
   </children>
</AnchorPane>
