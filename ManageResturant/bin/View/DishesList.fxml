<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.DishesListController">
   <children>
      <AnchorPane fx:id="dishesList" layoutX="2.0" prefHeight="524.0" prefWidth="526.0">
         <children>
            <ImageView fitHeight="533.0" fitWidth="534.0" layoutX="-4.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/1.jpg" />
               </image>
            </ImageView>
            <Button fx:id="back" layoutX="29.0" layoutY="22.0" mnemonicParsing="false" onAction="#dishesListSceneToMainScene" prefHeight="25.0" prefWidth="61.0" text="Back" />
            <Label layoutX="183.0" layoutY="20.0" prefHeight="55.0" prefWidth="134.0" text="Dishes:" textFill="#efb81e">
               <font>
                  <Font name="System Bold Italic" size="38.0" />
               </font>
            </Label>
            <ListView fx:id="allDishes" layoutX="14.0" layoutY="154.0" prefHeight="260.0" prefWidth="479.0" />
            <ImageView fitHeight="128.0" fitWidth="134.0" layoutX="382.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/logo.PNG" />
               </image>
            </ImageView>
            <Button layoutX="228.0" layoutY="434.0" mnemonicParsing="false" onAction="#editDish" onMouseClicked="#dishChoose" text="Edit Dish" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
