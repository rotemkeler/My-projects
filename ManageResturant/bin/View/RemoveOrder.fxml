<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.RemoveOrderController">
   <children>
      <AnchorPane fx:id="removeOrderScreen" layoutX="2.0" prefHeight="500.0" prefWidth="500.0">
         <children>
            <Label layoutX="142.0" layoutY="75.0" prefHeight="40.0" prefWidth="215.0" text="Remove Order:" textFill="#efb81e">
               <font>
                  <Font name="System Bold Italic" size="28.0" />
               </font>
            </Label>
            <Button fx:id="back" layoutX="29.0" layoutY="22.0" mnemonicParsing="false" onAction="#removeOrderSceneToMainScene" prefHeight="25.0" prefWidth="61.0" text="Back" />
            <Button layoutX="219.0" layoutY="426.0" mnemonicParsing="false" onAction="#saveRemoveOrder" prefHeight="25.0" prefWidth="61.0" text="Remove" />
            <Label layoutX="64.0" layoutY="150.0" prefHeight="49.0" prefWidth="434.0" text="Please select a order from the list:">
               <font>
                  <Font size="23.0" />
               </font>
            </Label>
            <ComboBox fx:id="ordersList" layoutX="84.0" layoutY="237.0" prefHeight="25.0" prefWidth="323.0" />
            <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="350.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/logo.PNG" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
