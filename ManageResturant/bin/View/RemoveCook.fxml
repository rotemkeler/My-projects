<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.RemoveCookController">
   <children>
      <AnchorPane fx:id="removeCookScreen" layoutY="4.0" prefHeight="500.0" prefWidth="500.0">
         <children>
            <Label layoutX="155.0" layoutY="75.0" prefHeight="40.0" prefWidth="203.0" text="Remove Cook:" textFill="#efb81e">
               <font>
                  <Font name="System Bold Italic" size="28.0" />
               </font>
            </Label>
            <Label layoutX="80.0" layoutY="154.0" prefHeight="49.0" prefWidth="434.0" text="Please select a cook from the list:">
               <font>
                  <Font size="23.0" />
               </font>
            </Label>
            <ComboBox fx:id="cooksList" layoutX="75.0" layoutY="243.0" prefHeight="25.0" prefWidth="364.0" />
            <Button fx:id="back" layoutX="14.0" layoutY="21.0" mnemonicParsing="false" onAction="#removeCookSceneToMainScene" prefHeight="25.0" prefWidth="65.0" text="Back" />
            <Button layoutX="224.0" layoutY="436.0" mnemonicParsing="false" onAction="#saveRemoveCook" prefHeight="25.0" prefWidth="65.0" text="Remove" />
            <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="364.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/logo.PNG" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
