<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.GetCooksByExpertiseController">
   <children>
      <AnchorPane fx:id="cooksByExpertise" layoutX="1.0" prefHeight="500.0" prefWidth="500.0">
         <children>
            <Button fx:id="back" layoutX="29.0" layoutY="22.0" mnemonicParsing="false" onAction="#cooksByExpertiseSceneToMainScene" prefHeight="25.0" prefWidth="61.0" text="Back" />
            <Label layoutX="104.0" layoutY="61.0" prefHeight="40.0" prefWidth="263.0" text="Cooks By Expertise:" textFill="#efb81e">
               <font>
                  <Font name="System Bold Italic" size="28.0" />
               </font>
            </Label>
            <ComboBox fx:id="expertiseList" editable="true" layoutX="149.0" layoutY="174.0" prefHeight="25.0" prefWidth="189.0" promptText="select" />
            <Label layoutX="175.0" layoutY="133.0" prefHeight="26.0" prefWidth="150.0" text="Choose Expertise:">
               <font>
                  <Font name="System Bold" size="17.0" />
               </font>
            </Label>
            <Label layoutX="29.0" layoutY="250.0" prefHeight="26.0" prefWidth="150.0" text="Cooks LIst:" textFill="#e5a417">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <Button layoutX="209.0" layoutY="217.0" mnemonicParsing="false" onAction="#showCooksList" prefHeight="25.0" prefWidth="69.0" text="Continue" />
            <ListView fx:id="cooksList" layoutX="29.0" layoutY="276.0" prefHeight="200.0" prefWidth="443.0" />
            <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="350.0" layoutY="-4.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/logo.PNG" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
