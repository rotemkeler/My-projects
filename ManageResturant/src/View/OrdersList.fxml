<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.OrdersListController">
   <children>
      <AnchorPane fx:id="ordersList" layoutX="1.0" prefHeight="500.0" prefWidth="500.0">
         <children>
            <ImageView fitHeight="517.0" fitWidth="533.0" layoutX="-5.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/1.jpg" />
               </image>
            </ImageView>
            <Button fx:id="back" layoutX="29.0" layoutY="22.0" mnemonicParsing="false" onAction="#ordersListSceneToMainScene" prefHeight="25.0" prefWidth="61.0" text="Back" />
            <Label layoutX="178.0" layoutY="20.0" prefHeight="55.0" prefWidth="142.0" text="Orders:" textFill="#efb81e">
               <font>
                  <Font name="System Bold Italic" size="38.0" />
               </font>
            </Label>
            <ListView fx:id="allOrders" layoutX="22.0" layoutY="145.0" prefHeight="254.0" prefWidth="469.0" />
            <ImageView fitHeight="136.0" fitWidth="127.0" layoutX="374.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/logo.PNG" />
               </image>
            </ImageView>
            <Button layoutX="228.0" layoutY="419.0" mnemonicParsing="false" onAction="#editOrder" onMouseClicked="#orderChoose" text="Edit Order" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
