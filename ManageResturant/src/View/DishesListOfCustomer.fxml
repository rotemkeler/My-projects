<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.DishesListOfCustomerController">
   <children>
      <AnchorPane fx:id="allDishesOfCustomer" prefHeight="500.0" prefWidth="500.0">
         <children>
            <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="350.0" layoutY="-4.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/logo.PNG" />
               </image>
            </ImageView>
            <Label layoutX="187.0" layoutY="71.0" prefHeight="50.0" prefWidth="125.0" text="Dishes:" textFill="#efb81e">
               <font>
                  <Font name="System Bold Italic" size="34.0" />
               </font>
            </Label>
            <Button layoutX="22.0" layoutY="21.0" mnemonicParsing="false" onAction="#backToOrders" prefHeight="25.0" prefWidth="55.0" text="Back" />
            <ListView fx:id="allDishes" layoutX="51.0" layoutY="150.0" onMouseClicked="#dishChoose" prefHeight="259.0" prefWidth="398.0" />
            <Button layoutX="214.0" layoutY="431.0" mnemonicParsing="false" onAction="#editDishes" prefHeight="25.0" prefWidth="72.0" text="Edit Dish" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
