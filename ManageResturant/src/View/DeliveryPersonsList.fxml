<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.DpListController">
   <children>
      <AnchorPane fx:id="deliveryPersonsList" prefHeight="500.0" prefWidth="500.0">
         <children>
            <ImageView fitHeight="529.0" fitWidth="524.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/1.jpg" />
               </image>
            </ImageView>
            <Button fx:id="back" layoutX="18.0" layoutY="17.0" mnemonicParsing="false" onAction="#dpListSceneToMainScene" prefHeight="25.0" prefWidth="61.0" text="Back" />
            <Label layoutX="95.0" layoutY="29.0" prefHeight="55.0" prefWidth="312.0" text="Delivery Persons:" textFill="#efb81e">
               <font>
                  <Font name="System Bold Italic" size="35.0" />
               </font>
            </Label>
            <ListView fx:id="allDeliveryPersons" layoutX="18.0" layoutY="132.0" prefHeight="257.0" prefWidth="464.0" />
            <ImageView fitHeight="122.0" fitWidth="121.0" layoutX="383.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/logo.PNG" />
               </image>
            </ImageView>
            <Button layoutX="236.0" layoutY="413.0" mnemonicParsing="false" onAction="#editDp" onMouseClicked="#dpChoose" text="Edit Dp" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
